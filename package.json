    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "web-streams-polyfill": "^4.1.0",
    "whatwg-fetch": "^3.6.20"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
